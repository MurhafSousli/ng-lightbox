{
  "name": "@ngx-gallery/demo",
  "description": "Angular image gallery simplifies the process of creating beautiful image gallery for the web and mobile devices.",
  "version": "2.2.1",
  "homepage": "http://github.com/murhafsousli/ng-gallery",
  "author": {
    "name": "Murhaf Sousli",
    "url": "http://github.com/murhafsousli"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/murhafsousli/ng-gallery.git"
  },
  "license": "MIT",
  "keywords": [
    "angular",
    "ionic",
    "gallery",
    "images",
    "lightbox",
    "modal",
    "slider",
    "overlay",
    "carousel"
  ],
  "bugs": {
    "url": "http://github.com/murhafsousli/ng-gallery/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p lib/ng-package.json && npm run sass",
    "sass-ts": "tsc .config/styles.ts --allowJs && node .config/styles.js lib/core/src build/core",
    "sass": "node .config/styles.js lib/core/src build/core",
    "prepare-build": "npm run build-core && npm run version && npm run link-core && npm run build-lightbox && npm run version && npm run link-lightbox && npm run build-gallerize && npm run version && npm run link-gallerize && npm run sass && npm run version",
    "build-all": "npm run build-core && npm run build-lightbox && npm run build-gallerize && npm run sass && npm run version",
    "build-core": "ng-packagr -p lib/core/ng-package.json",
    "build-lightbox": "ng-packagr -p lib/lightbox/ng-package.json",
    "build-gallerize": "ng-packagr -p lib/gallerize/ng-package.json",
    "link-core": "npm link build/core && npm link @ngx-gallery/core",
    "link-lightbox": "npm link build/lightbox && npm link @ngx-gallery/lightbox",
    "link-gallerize": "npm link build/gallerize && npm link @ngx-gallery/gallerize",
    "publish-all": "node .config/publish.js",
    "version": "node .config/build.js",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles": "ng build --prod && ng build --prod --app 1 --output-hashing=false",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "6.0.5",
    "@angular/cdk": "^6.0.0",
    "@angular/common": "6.0.5",
    "@angular/compiler": "6.0.5",
    "@angular/core": "6.0.5",
    "@angular/forms": "6.0.5",
    "@angular/http": "6.0.5",
    "@angular/material": "^6.0.0",
    "@angular/platform-browser": "6.0.5",
    "@angular/platform-browser-dynamic": "6.0.5",
    "@angular/platform-server": "6.0.5",
    "@angular/router": "6.0.5",
    "@nguniversal/module-map-ngfactory-loader": "6.0.0",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "rxjs": "^6.2.1",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "6.0.8",
    "@angular/compiler-cli": "6.0.5",
    "@angular/language-service": "6.0.5",
    "@types/bluebird": "^3.5.20",
    "@types/core-js": "0.9.36",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "7.0.7",
    "@types/node-sass": "^3.10.32",
    "ansi-colors": "2.0.1",
    "autoprefixer": "^8.5.1",
    "codelyzer": "~4.3.0",
    "cssnano": "^3.10.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0",
    "node-sass": "^4.9.0",
    "postcss": "^6.0.22",
    "postcss-strip-inline-comments": "^0.1.5",
    "protractor": "~5.1.2",
    "replace-ext": "^1.0.0",
    "ts-loader": "^3.5.0",
    "ts-node": "^6.0.5",
    "tslint": "~5.7.0",
    "typescript": "2.7.2",
    "@angular-devkit/build-angular": "~0.6.8"
  }
}
